<div id="gsap-box" style="width: 100px; height: 100px; background: coral;"></div>

<script>
  const box = document.getElementById('gsap-box');
  
  if (box) {
    import('gsap').then(({ gsap }) => {
      gsap.fromTo(box, 
      {x: -200},
      {
        x: 200,
        duration: 2,
        repeat: -1,
        yoyo: true,
        ease: 'power1.inOut',
      });
    });
  }
</script>